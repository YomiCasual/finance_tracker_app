 <tr class="border-b"  id="stock_list_<%= stock.id %>" >
    <td class="tbody-text">
		<%= stock.ticker  %>
	</td>
    <td class="tbody-text">
		<%= stock.name  %>
	</td>
    <td class="tbody-text">
		<%= stock&.last_price  %>
	</td>
	<td class="tbody-text">

	<%= turbo_frame_tag 'portfolio' do %>
	<%= button_to "Delete", user_stock_path(id: stock.id), class: "btn-danger !mt-4 !w-max", method: :delete,
	 data: { turbo_confirm: "Are you sure you want remove this stock?"}
	%>
	<% end %>
	</td>
</tr>